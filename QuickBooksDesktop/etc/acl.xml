<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magenest_QuickBooksDesktop::integration" title="Manage QuickBooksDesktop" sortOrder="60">
                    <resource id="Magenest_QuickBooksDesktop::queue_menu" title="Queue Menu">
                        <resource id="Magenest_QuickBooksDesktop::queue" title="Queue Manager" sortOrder="20"/>
                    </resource>

                    <resource id="Magenest_QuickBooksDesktop::mapping" title="Mapping Settings">
                        <resource id="Magenest_QuickBooksDesktop::manage_tax" title="Tax Management" sortOrder="20"/>
                    </resource>

                    <resource id="Magenest_QuickBooksDesktop::configuration" title="Configuration">
                        <resource id="Magenest_QuickBooksDesktop::user" title="User" sortOrder="20"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magenest_QuickBooksDesktop::config_qbdesktop" title="Quickbook Setting Section"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
