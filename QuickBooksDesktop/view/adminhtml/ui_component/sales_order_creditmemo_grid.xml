<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="magenest_add_credit_memo_to_queue">
                <settings>
                    <url path="qbdesktop/creditMemo/massAddCreditMemoToQueue"/>
                    <type>magenest_add_credit_memo_to_queue</type>
                    <label translate="true">Add Credit Memo(s) to Queue</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>