<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="magenest_add_to_queue">
                <settings>
                    <confirm>
                        <message translate="true">Are you sure you want to add the selected customers to Queue?</message>
                        <title translate="true">Add items to queue</title>
                    </confirm>
                    <url path="qbdesktop/customer/massAddCustomerToQueue"/>
                    <type>magenest_add_to_queue</type>
                    <label translate="true">Add To Queue</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>